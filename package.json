{
  "name": "jsonl-gazelle",
  "displayName": "JSONL Gazelle",
  "description": "Fast JSONL viewer / editor for VS Code with advanced features including Table View and Pretty Print view.",
  "version": "0.3.1",
  "publisher": "gabor",
  "icon": "jsonl-gazelle.png",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Data Science",
    "Other"
  ],
  "keywords": [
    "jsonl",
    "json",
    "data",
    "table",
    "viewer",
    "csv",
    "export",
    "ai",
    "openai"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gaborcselle/jsonl-gazelle.git"
  },
  "homepage": "https://github.com/gaborcselle/jsonl-gazelle",
  "bugs": {
    "url": "https://github.com/gaborcselle/jsonl-gazelle/issues"
  },
  "license": "MIT",
  "activationEvents": [
    "onStartupFinished",
    "onCustomEditor:jsonl-gazelle.jsonlViewer",
    "onCommand:jsonl-gazelle.openLargeFile"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "jsonl-gazelle.jsonlViewer",
        "displayName": "JSONL Gazelle",
        "selector": [
          {
            "filenamePattern": "*.jsonl"
          }
        ],
        "priority": "default"
      }
    ],
    "commands": [
      {
        "command": "jsonl-gazelle.openSettings",
        "title": "Open Settings",
        "category": "JSONL Gazelle"
      },
      {
        "command": "jsonl-gazelle.exportCSV",
        "title": "Export to CSV",
        "category": "JSONL Gazelle"
      },
      {
        "command": "jsonl-gazelle.openLargeFile",
        "title": "Split into Parts (100MB+)",
        "category": "JSONL Gazelle",
        "icon": "$(file)"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "jsonl-gazelle.openSettings",
          "when": "resourceExtname == .jsonl"
        },
        {
          "command": "jsonl-gazelle.exportCSV",
          "when": "resourceExtname == .jsonl"
        },
        {
          "command": "jsonl-gazelle.openLargeFile",
          "when": "resourceExtname == .jsonl"
        }
      ],
      "explorer/context": [
        {
          "command": "jsonl-gazelle.openLargeFile",
          "when": "resourceExtname == .jsonl",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
}
